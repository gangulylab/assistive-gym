<?xml version="1.0" ?>

<robot name="laptop.urdf">
  <material name="Dark Grey">
    <color rgba=".3 .3 .3 1"/>
  </material>
  <material name="Light Grey">
    <color rgba=".5 .5 .5 1"/>
  </material>
  <link name="frame_link">
    <!-- <contact>
      <lateral_friction value="1.0"/>
    </contact> -->
    <inertial>
      <mass value="100"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy = "0 0 0"/>
      <geometry>
			  <mesh filename="slider_frame.obj"/>
      </geometry>
      <material name="Dark Grey"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy = "0 0 0"/>
      <geometry>
			  <mesh filename="slider_frame.obj"/>
      </geometry>
    </collision>
    
  </link>
  <link name="door_link">
    <!-- <contact>
      <lateral_friction value="1.0"/>
    </contact> -->
    <inertial>
      <mass value="100"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
			  <mesh filename="slider_door.obj"/>
      </geometry>
      <material name="Light Grey"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
			  <mesh filename="slider_door.obj"/>
      </geometry>
    </collision>
  </link>
  <joint name="hinge" type="prismatic">
      <parent link="frame_link"/>
      <child link="child_link"/>
      <axis xyz="1 0 0"/>
      <limit lower="0" upper=".1" effort="100" velocity="100"/>
      <dynamics dampling="0" friction=".1"/>
  </joint>
</robot>